      "data": [
        {
          "id": 0,
          "from": "client",
          "text": "Comment faire pour activer ma carte Max suite à une opposition ?",
          "intentDefinition": {
            "label": null,
            "name": "comment_fa",
            "category": "build",
            "description": null,
            "intentId": "6294ce2ebabad268692173f4",
            "sentences": [
              "Comment faire pour activer ma carte ?",
              "Comment activer ma carte ?",
              "Je souhaite activer ma carte. Comment dois-je faire ?",
              "Comment activer ma carte Max ?",
              "J'ai une carte Max. Comment l'activer ?"
            ]
          }
        },
        {
          "id": 1,
          "parentIds": [
            0
          ],
          "from": "api",
          "text": "Recup identifiant Topaze.",
          "apiCallDefinition": {
            "id": 1,
            "parentIds": [
              0
            ],
            "from": "api",
            "text": "Recup identifiant Topaze."
          }
        },
        {
          "id": 2,
          "parentIds": [
            1
          ],
          "from": "bot",
          "text": "Souhaitez-vous :\n- Activer une nouvelle carte max ?\n\n- Ou activer une carte bancaire externe que vous avez ajoutée dans l'application ?"
