        },
        {
          "id": 4,
          "parentIds": [
            2
          ],
          "from": "client",
          "text": "Activer ma carte max",
          "intentDefinition": {
            "label": "Activer ma carte max",
            "name": "activermacartemax",
            "category": "scenarios",
            "description": "Activer carte Aumax",
            "sentences": [
              "Il s'agit d'une carte Max",
              "Max",
              "Carte max"
