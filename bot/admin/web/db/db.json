        {
          "id": 3,
          "parentIds": [
            2
          ],
          "from": "client",
          "text": "Activer carte bancaire externe",
          "intentDefinition": {
            "label": "Activer carte bancaire externe",
            "name": "activercartebancaireexterne",
            "category": "scenarios",
            "description": null,
            "sentences": [
              "Carte externe",
              "Carte Société générale",
              "Carte Banque postale",
              "Carte BNP",
              "Carte crédit agricole"
            ]
